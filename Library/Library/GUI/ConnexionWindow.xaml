<Window x:Class="Library.GUI.ConnexionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library.GUI"
        mc:Ignorable="d"
        Title="ConnexionWindow" Height="450" Width="800">
    <Grid>
        <!-- LibraryImage -->
        <Image HorizontalAlignment="Left" Height="705" VerticalAlignment="Top" Width="481" Source="/GUI/LibraryImage.jpg" Margin="-15,-123,0,0" RenderTransformOrigin="0.418,0.506" />
        <Label Content="Email" HorizontalAlignment="Left" Margin="535,162,0,0" VerticalAlignment="Top" Width="64"/>
        <Label Content="Se connecter" HorizontalAlignment="Left" Margin="478,38,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxEmail" HorizontalAlignment="Left" Margin="615,166,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="530,222,0,0" VerticalAlignment="Top" Width="65"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Left" Margin="615,228,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnSeConnecter" Content="Se connecter" HorizontalAlignment="Left" Margin="591,309,0,0" VerticalAlignment="Top" Click="SeConnecter"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="535,371,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</Window>
